/**
输入: 1->2->3->4->5->NULL
输出: 5->4->3->2->1->NULL
**/

    
    
   public ListNode reverseList(ListNode head) {
        ListNode pre = null;
        //pre.next = head;
        ListNode curr = head;
        
        while(curr != null){
            ListNode temp = curr.next;
            curr.next = pre;
            pre = curr;//不能写pre = pre.next
            curr = temp;//不能写curr = curr.next;
        }
    return pre;
    }
}
